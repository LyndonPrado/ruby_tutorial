<!DOCTYPE html>
<html>
  <head>
    <title><%= @page_title %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <link href="https://fonts.googleapis.com/css?family=Lato|Roboto&display=swap" rel="stylesheet"> 

    <meta name="keywords" content="<%= @seo_keywords %>" />
    <%= favicon_link_tag 'favicons/other_favicon.ico' %>
    <%= stylesheet_link_tag 'blogs', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <!-- Start of nav -->
    <%= render 'shared/blog_nav' %>
    <!-- End of nav -->

    <!-- start of blog header -->
     <%= render 'shared/blog_masthead' %>
    <!-- end of blog header -->


    <!-- start of container and row element -->
    <div class="container">
      <div class="row">
        <div class="col-sm-7 ">
        <div class="row">

      <!-- start of container and row element -->
        <!--   <%= login_helper%> -->
          <%= yield %>

        </div>
        </div>
     
    <%= render 'shared/blog_sidebar' %>
      </div>
    <!-- start of footer -->
    <%= render 'shared/blog_footer' %>
    <!-- end of footter -->



            <%= render "shared/nav", location: 'bottom' %>

            <%= alerts %>
            
    </div>
  </body>
</html>
